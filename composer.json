{
  "name": "hlev/jetstrap-minimal",
  "description": "A package for Laravel 10 and 11 to replace TailwindCSS with Bootstrap 5.3, Livewire + Blade, Teams",
  "keywords": [
    "laravel",
    "jetstream",
    "php",
    "blade",
    "livewire",
    "bootstrap"
  ],
  "require": {
    "php": "^8.1",
    "illuminate/contracts": "^10.0||^11.0",
    "spatie/laravel-package-tools": "^1.16.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Hlev\\JetstrapMinimal\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Levente Huszko",
      "email": "levente.huszko@high-end.hu"
    }
  ],
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Hlev\\JetstrapMinimal\\JetstrapMinimalServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  }
}
